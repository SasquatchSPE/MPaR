@model List<MPR.Models.Games.Game>
<table class="table" id="example">
    <tbody>
        @foreach (var game in Model)
        {
        <tr>

            @if (@game.AwayTeamLink == null)
            {
                <td>@game.AwayTeam</td>
            }
            else
            {
                <td><a href="@game.AwayTeamLink">@game.AwayTeam</a></td>
            }

            @if (@game.NotifyAway)
            {
                <td style="font-weight: bold">@game.AwayTeamScore</td>
            }
            else
            {
                <td>@game.AwayTeamScore</td>
            }

            @if (@game.HomeTeamLink == null)
            {
                <td>@game.HomeTeam</td>
            }
            else
            {
                <td><a href="@game.HomeTeamLink">@game.HomeTeam</a></td>
            }

            @if (@game.NotifyHome)
            {
                <td style="font-weight: bold">@game.HomeTeamScore</td>
            }
            else
            {
                <td>@game.HomeTeamScore</td>
            }

            <td>
            @if (@game.TimeLink == null)
            {
                @game.Time
            }
            else
            {
                <a href="@game.TimeLink">@game.Time</a>
            }
            @if(game.LiveLink != null)
            {
                <span> | </span><a href="@game.LiveLink">Twitch</a>
            }
            </td>

        </tr>
        }
    </tbody>
</table>
